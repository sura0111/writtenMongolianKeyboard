{
  "name": "written-mongol-keyboard",
  "version": "1.2.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "yarn clear && yarn compile",
    "clear": "rimraf dist",
    "compile": "webpack && cp -R @types dist",
    "lint": "eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "postpack": "yarn clear",
    "postpublish": "yarn clear",
    "prepack": "yarn build",
    "prepublish": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": "git@github.com:sura0111/writtenMongolianKeyboard.git",
  "author": "sura0111 <sura0111@gmail.com>",
  "license": "MIT",
  "keywords": ["traditionalMongolian", "writtenMongolian", "keyboard", "language"],
  "dependencies": {
    "@types/textarea-caret": "^3.0.0",
    "textarea-caret": "^3.1.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint": "^7.15.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.11",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tsconfig-paths": "^3.9.0",
    "webpack-cli": "^4.2.0",
    "webpack": "^5.10.0"
  }
}
